{
  "mainBroker": {
    "host": "localhost",
    "port": "2181",
    "topics": ["topic1", "topic2"],
    "options": {
      "groupId": "groupOne",
      "sessionTimeout": 15000,
      "protocol": ["roundrobin"],
      "fromOffset": "latest"
    },
    "adapter": "strong-pubsub-kafka",
    "handler": "./mainBrokerHandler"
  }
}